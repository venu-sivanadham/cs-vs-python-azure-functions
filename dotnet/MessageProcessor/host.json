{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  },
  "extensions": {
    "queues": {
      "batchSize": 32,
      "maxPollingInterval": "00:00:01",
      "newBatchThreshold": 6
    }
  }
}