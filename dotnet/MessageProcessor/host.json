{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "logLevel": {
      "default": "Information",
      "Host.Results": "Error",
      "Function": "Error",
      "Host.Aggregator": "Information"
    }
  },
  "extensions": {
    "queues": {
      "batchSize": 32,
      "maxPollingInterval": "00:00:01",
      "newBatchThreshold": 6
    }
  }
}